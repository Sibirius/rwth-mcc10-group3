<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Hello Maps">
    <Require feature="wave" /> 
  </ModulePrefs>
  <Content type="html">
 	<![CDATA[
 		<script src="http://maps.google.com/maps?file=api&amp;v=2&amp;sensor=false&amp;key=ABQIAAAAGAP5T4BS8Gn8z_KNdk4MuxQs3QBYDBscCkXn5XVh-PO2erUf9BRLbMyRF04Gc0cJo5xmV5wt1Y7oPQ" type="text/javascript"></script>
		<script type="text/javascript"> 
	
	      // Display the map, with some controls and set the initial location 
	      var map = new GMap2(document.getElementById("map"));
	      map.addControl(new GLargeMapControl());
	      map.addControl(new GMapTypeControl());
	      map.setCenter(new GLatLng(50.77788,6.07826),8);
	    	
	    }
		</script>
    	<div id="map" style="width: 550px; height: 450px"></div>      
    ]]>
  </Content>
</Module>