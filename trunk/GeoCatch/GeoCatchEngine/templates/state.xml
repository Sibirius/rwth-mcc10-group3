<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wayne>
<state 
	mode ="{{mode}}"
	state="{{state}}"

	lat="{{modespecific.lat}}"
	lon="{{modespecific.lon}}"	
/>

<events>
	{% for event in events %}
	<event 
		title="{{event.title}}"
		info="{{event.info}}"
		extra="{{event.extra}}"
	/>		
	{% endfor %}
</events>
</wayne>