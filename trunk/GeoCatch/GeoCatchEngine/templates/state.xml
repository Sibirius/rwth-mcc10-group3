<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<state 
	mode ="{{mode}}"
	state="{{state}}"
	
	{% ifequal mode "0" %} {% comment %} race TODO {% endcomment %}
	{% endifequal %}	
			
	{% ifequal mode "1" %} {% comment %} chaincatch {% endcomment %}
		lat="{{modespecific.lat}}"
		lon="{{modespecific.lon}}"
	{% endifequal %}
/>

<events>
	{% for event in events %}
	<event 
		title="{{event.title}}"
		info="{{event.info}}"
		extra="{{event.extra}}"
	/>		
	{% endfor %}
</events>
